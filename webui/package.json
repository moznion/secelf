{
  "name": "webui",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "moznion <moznion@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "lint": "tslint --project ./tsconfig.json --config ./.tslint.json ./src/**/*",
    "fmt_ts": "find src -name \"*.ts\" | xargs prettier --config .ts_prettierrc --write",
    "fmt_vue": "find src -name \"*.vue\" | xargs prettier --config .vue_prettierrc --write",
    "fmt": "npm run fmt_ts && npm run fmt_vue"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "bootstrap-vue": "^2.0.0-rc.11",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "ajv": "^6.5.4",
    "ajv-keywords": "^3.2.0",
    "cross-env": "^5.1.1",
    "css-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.5",
    "node-sass": "^4.9.4",
    "prettier": "^1.14.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.6",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
